{
  "name": "@gnosispay/account-kit",
  "version": "1.0.0",
  "author": "cristovao.honorato@gnosis.io",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/cjs/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js"
    }
  },
  "files": [
    "package.json",
    "LICENSE",
    "README.md",
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf dist build typechain-types",
    "build": "hardhat compile && tsc -p tsconfig.cjs.json && tsc -p tsconfig.esm.json",
    "test": "hardhat test",
    "lint": "eslint 'scripts/**/*.ts' 'src/**/*.ts' 'test/**/*.ts' --max-warnings 0 --fix",
    "format": "prettier ./scripts/**/*.ts ./src/**/*.ts ./test/**/*.ts ./*.ts -w",
    "interfaces": "ts-node ./scripts/generateInterfaces.ts",
    "prepare": "yarn run clean && yarn interfaces && yarn run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gnosispay/account-kit"
  },
  "devDependencies": {
    "@gnosis.pm/safe-contracts": "1.3.0",
    "@nomicfoundation/hardhat-chai-matchers": "2.0.2",
    "@nomicfoundation/hardhat-ethers": "^3.0.4",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.7",
    "@nomicfoundation/hardhat-toolbox": "3.0.0",
    "@nomicfoundation/hardhat-verify": "^1.1.1",
    "@safe-global/safe-deployments": "^1.26.0",
    "@safe-global/safe-singleton-factory": "^1.0.15",
    "@typechain/ethers-v6": "^0.5.0",
    "@typechain/hardhat": "^9.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.5.6",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "abi-to-sol": "^0.8.0",
    "chai": "^4.3.7",
    "dotenv": "^16.0.3",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^5.0.0",
    "hardhat": "^2.12.5",
    "hardhat-gas-reporter": "^1.0.9",
    "prettier": "^3.0.2",
    "prettier-plugin-solidity": "^1.1.3",
    "rimraf": "^5.0.1",
    "solidity-coverage": "^0.8.2",
    "ts-node": "^10.9.1",
    "typechain": "^8.1.1",
    "typescript": "5.1.6"
  },
  "dependencies": {
    "@gnosis.pm/zodiac": "^3.3.7",
    "@safe-global/safe-deployments": "^1.26.0",
    "@safe-global/safe-modules-deployments": "^1.1.0",
    "ethers": "^6.7.1"
  }
}
